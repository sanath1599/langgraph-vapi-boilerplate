AZURE_OPENAI_ENDPOINT=
PORT=6000
CLINIC_TIMEZONE=America/New_York   # IANA timezone for slot times (default: UTC)

# Appointment backend API key (sent as x-api-key when backend REQUIRE_API_KEY=true) (Generate using backend)
APPOINTMENT_API_KEY=
# Or use MOCK_API_KEY= for the same purpose
# Your Azure OpenAI API key
AZURE_OPENAI_API_KEY=

# Your Azure OpenAI deployment name (the model deployment you created in Azure)
AZURE_OPENAI_DEPLOYMENT_NAME=

# Azure OpenAI API version
AZURE_OPENAI_API_VERSION=

# LangSmith
LANGSMITH_API_KEY=
LANGSMITH_TRACING_V2=
LANGSMITH_PROJECT=appointment
 
# Resk (resk-llm-ts) - set RESK_ENABLED=false to bypass security and call Azure directly
RESK_ENABLED=false
RESK_PROMPT_INJECTION_LEVEL=basic
RESK_PII_REDACT=true
RESK_CONTENT_MODERATION_SEVERITY=medium
RESK_HEURISTIC_ENABLED=true
RESK_CANARY_TOKENS_ENABLED=true
RESK_INPUT_SANITIZATION_ENABLED=true